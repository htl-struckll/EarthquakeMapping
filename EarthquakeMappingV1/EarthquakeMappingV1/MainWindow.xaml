<Window x:Class="EarthquakeMappingV1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EarthquakeMappingV1"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="EarhquakeMapping" Height="640" Width="800">
    <Grid>
        <Grid x:Name="MapGrid" Margin="45,10,44,10">
            <Grid.Background>
                <ImageBrush ImageSource="Pics/mercator_map.jpg" Stretch="UniformToFill"/>
            </Grid.Background>
        </Grid>
    </Grid>
</Window>
